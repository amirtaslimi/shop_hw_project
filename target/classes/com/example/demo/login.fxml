<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="412.0" prefWidth="517.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button fx:id="login" layoutX="296.0" layoutY="304.0" onAction="#login" text="login" />
   <TextField fx:id="username" layoutX="193.0" layoutY="101.0" />
   <PasswordField fx:id="password" layoutX="194.0" layoutY="175.0" />
   <MenuButton layoutX="315.0" layoutY="227.0" mnemonicParsing="false" text="kind">
     <items>
       <MenuItem mnemonicParsing="false" onAction="#userCh" text="user" />
       <MenuItem mnemonicParsing="false" onAction="#sellerCh" text="seller" />
     </items>
   </MenuButton>
   <Button fx:id="register" layoutX="193.0" layoutY="304.0" onAction="#register" text="register" />
   <Label layoutX="193.0" layoutY="82.0" prefHeight="18.0" prefWidth="150.0" text="please enter your username" />
   <Label layoutX="192.0" layoutY="156.0" prefHeight="18.0" prefWidth="150.0" text="please enter your password" />
   <Label layoutX="146.0" layoutY="230.0" prefHeight="18.0" prefWidth="150.0" text="please choose your kind" />
   <Label fx:id="liveStatus" layoutX="113.0" prefHeight="74.0" prefWidth="290.0" text="status: please enter your information" />
</AnchorPane>
